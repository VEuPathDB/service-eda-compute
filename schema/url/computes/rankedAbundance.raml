#%RAML 1.0 Library

uses:
  eda: ../eda-common-lib.raml

types:
  RankedAbundancePluginRequest:
    type: eda.ComputeRequestBase
    properties:
      config: RankedAbundancePluginConfig

  RankedAbundancePluginConfig:
    type: object
    additionalProperties: false
    properties:
      outputEntityId:
        type: string
      collectionVariable: eda.VariableSpec
      rankingMethod: RankingMethod

  RankingMethod:
    type: string
    enum: ['median', 'q3', 'variance', 'max']
