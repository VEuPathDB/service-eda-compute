#%RAML 1.0 Library

types:

  BetaDivPluginRequest:
    type: ComputeRequestBase
    properties:
      config: BetaDivComputeConfig

  BetaDivComputeConfig:
    properties:
      collectionVariable: VariableSpec
      betaDivDistanceMethod: BetaDivDistanceMethod

  BetaDivDistanceMethod:
    type: string
    # RD: what happened to these (were in data service)? 'unifrac', 'wunifrac', 'jensen-shannon'
    enum: ['bray', 'jaccard', 'jsd']

  BetaDivStats:
    properties:
      pcoaVariance: number[]
